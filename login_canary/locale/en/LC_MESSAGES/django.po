#: apps.py:10
msgid "Login Canary"
msgstr ""

#: templates/login_canary/logged_in_body.txt:1
#, python-format
msgid ""
"Dear %(user)s,\n"
"\n"
"On %(datetime)s somebody successfully logged in to your account on "
"%(service_name)s.\n"
"\n"
"User agent: %(user_agent)s\n"
"IP-Address: %(remote_addr)s\n"
"\n"
"Kind regards."
msgstr ""

#: templates/login_canary/logged_in_subject.txt:1
#, python-format
msgid "Successful login on %(service_name)s"
msgstr ""

#: templates/login_canary/login_failed_body.txt:1
#, python-format
msgid ""
"Dear %(user)s,\n"
"\n"
"On %(datetime)s somebody tried to login to your account on %(service_name)s "
"but provided invalid credentials.\n"
"\n"
"Kind regards."
msgstr ""

#: templates/login_canary/login_failed_subject.txt:1
#, python-format
msgid "Failed login attempt on %(service_name)s"
msgstr ""
